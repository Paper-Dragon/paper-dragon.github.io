{
  "name": "cloudnative-status",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "deploy": "wrangler deploy",
    "dev": "wrangler dev",
    "start": "wrangler dev",
    "select": "wrangler d1 execute d1db --remote --command=\"SELECT * FROM dockerratelimitLimit ORDER BY update_time DESC LIMIT 10\" ",
    "exportdb": "wrangler d1 export d1db --remote --output \"./sql/old.sql\"",
    "importdb": "wrangler d1 execute d1db --remote --file=\"./sql/old.sql\"",
    "initdb": "wrangler d1 execute d1db --remote --file=\"./sql/schema.sql\""
  },
  "devDependencies": {
    "@cloudflare/vitest-pool-workers": "0.5.14",
    "vitest": "2.1.2",
    "wrangler": "^3.80.0"
  }
}